<%= render "devise/shared/header" %>
<form>
  <h1 class="login-h1">ログイン</h1>
  <div class="row mb-3">
    <label for="inputEmail3" class="label-change">メールアドレス</label>
    <input type="email" class="form-change form-control" id="inputEmail3" placeholder="店舗用アドレスを入力">
  </div>
  <div class="row mb-3">
    <label for="password" class="label-change">パスワード(6文字以上)</label>
    <input type="password" class="form-change form-control" id="password" placeholder="6文字以上の半角英数字">
  </div>
  <button type="submit" class="sign-in" >ログイン</button>
</form>


